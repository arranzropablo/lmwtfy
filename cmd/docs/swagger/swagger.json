{
    "swagger": "2.0",
    "info": {
        "description": "This is a simple API which gets a thing to search and returns the URL for the wiki search.",
        "title": "LMWTFY API doc",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "Pablo Arranz Ropero",
            "url": "http://arranzropablo.com",
            "email": "hi@arranzropablo.com"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "1.0"
    },
    "host": "{{.Host}}",
    "basePath": "{{.BasePath}}",
    "paths": {
        "/search": {
            "post": {
                "description": "Creates an user",
                "consumes": [
                    "application/x-www-form-urlencoded"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Create user",
                "parameters": [
                    {
                        "description": "The string you want to search by",
                        "name": "main",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "$ref": "#/definitions/search.body"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Everything went OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "search.body": {
            "type": "object",
            "properties": {
                "search": {
                    "type": "string",
                    "example": "madgyvers"
                }
            }
        }
    }
}